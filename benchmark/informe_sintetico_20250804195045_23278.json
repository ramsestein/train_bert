      "start": 605,
      "end": 617
    },
    {
      "text": "4 mm",
      "type": "MEDIDA",
      "start": 716,
      "end": 720
    },
    {
      "text": "hiperdensidad heterogénea compatible con sangre fresca",
      "type": "PATOLOGIA",
      "start": 734,
      "end": 788
    },
    {
      "text": "borramiento parcial de surcos corticales circundantes",
      "type": "PATOLOGIA",
      "start": 820,
      "end": 873
    },
    {
      "text": "No se identifican hemorragias subaracnoideas ni intraventriculares",
      "type": "HALLAZGO_NEGATIVO",
      "start": 875,
      "end": 941
    },
    {
      "text": "Sistema ventricular desplazado y ligeramente comprimido en el lado afectado",
      "type": "PATOLOGIA",
      "start": 943,
      "end": 1018
    },
    {
      "text": "sin signos de hidrocefalia",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1020,
      "end": 1046
    },
    {
      "text": "disminución de flujo cerebral (CBF) y volumen cerebral (CBV)",
      "type": "PATOLOGIA",
      "start": 1072,
      "end": 1132
    },
    {
      "text": "aumento del tiempo de tránsito medio (MTT)",
      "type": "PATOLOGIA",
      "start": 1162,
      "end": 1204
    },
    {
      "text": "hipoperfusión secundaria al efecto de masa y edema",
      "type": "PATOLOGIA",
      "start": 1220,
      "end": 1270
    },
    {
      "text": "No se identifican áreas de penumbra isquémica fuera del territorio afectado por el hematoma",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1272,
      "end": 1363
    },
    {
      "text": "Arterias principales permeables",
      "type": "PATOLOGIA",
      "start": 1398,
      "end": 1429
    },
    {
      "text": "sin signos evidentes de malformaciones vasculares ni aneurismas",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1430,
      "end": 1493
    },
    {
      "text": "No se identifican signos de vasoespasmo ni trombosis arterial",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1495,
      "end": 1556
    },
    {
      "text": "Hematoma intracerebral en núcleo caudado izquierdo",
