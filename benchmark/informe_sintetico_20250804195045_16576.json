  "document_id": "informe_sintetico_20250804195045_16576",
  "entities": [
    {
      "text": "edema cerebral difuso",
      "type": "PATOLOGIA",
      "start": 542,
      "end": 563
    },
    {
      "text": "pérdida bilateral de diferenciación cortico-subcortical",
      "type": "PATOLOGIA",
      "start": 568,
      "end": 623
    },
    {
      "text": "más pronunciado en núcleos basales y médula cerebral",
      "type": "LOCALIZACION_ANATOMICA",
      "start": 625,
      "end": 677
    },
    {
      "text": "Leve borramiento de surcos corticales",
      "type": "PATOLOGIA",
      "start": 744,
      "end": 781
    },
    {
      "text": "discreta disminución del tamaño ventricular",
      "type": "PATOLOGIA",
      "start": 784,
      "end": 827
    },
    {
      "text": "edema cerebral global",
      "type": "PATOLOGIA",
      "start": 845,
      "end": 866
    },
    {
      "text": "No se identifican hemorragias ni colecciones extraaxiales",
      "type": "HALLAZGO_NEGATIVO",
      "start": 868,
      "end": 925
    },
    {
      "text": "disminución global del flujo cerebral (CBF)",
      "type": "PATOLOGIA",
      "start": 1057,
      "end": 1100
    },
    {
      "text": "prolongación significativa del tiempo de tránsito medio (MTT)",
      "type": "PATOLOGIA",
      "start": 1135,
      "end": 1196
    },
    {
      "text": "tiempo hasta el pico (TTP)",
      "type": "PATOLOGIA",
      "start": 1199,
      "end": 1225
    },
    {
      "text": "sin área focal definida de \"core\" isquémico ni penumbra",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1329,
      "end": 1384
    },
    {
      "text": "No se visualizan signos de oclusión arterial intracraneal",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1421,
      "end": 1478
    },
    {
      "text": "ni estenosis significativa en las arterias cerebrales principales (ACAs, ACMs, ACPs)",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1479,
      "end": 1563
    },
    {
      "text": "Sistema vertebro-basilar permeable",
      "type": "PATOLOGIA",
      "start": 1565,
      "end": 1599
    },
    {
      "text": "Signos radiológicos compatibles con encefalopatía hipóxico-isquémica de distribución global",
      "type": "PATOLOGIA",
      "start": 1695,
      "end": 1786
    },
    {
      "text": "edema cerebral difuso y alteraciones en perfusión cerebral indicativas de compromiso hemodinámico difuso",
      "type": "PATOLOGIA",
      "start": 1792,
      "end": 1896
    },
    {
      "text": "sin evidencia de infarto focal o lesión vascular obstructiva",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1897,
      "end": 1957
    },
    {
      "text": "No se aprecian hemorragias ni desplazamientos de línea media",
      "type": "HALLAZGO_NEGATIVO",
      "start": 1959,
      "end": 2019
    },
    {
      "text": "TC CODIGO ICTUS (PERFUSION CEREBRAL+ ANGIOTC ARTERIAL INTRACRANEAL)",
      "type": "TECNICA_IMAGEN",
      "start": 2328,
      "end": 2395
    }
  ]
}