  "entities": [
    {
      "text": "disminución difusa y simétrica de la diferenciación cortico-subcortical",
      "type": "PATOLOGIA",
      "start": 414,
      "end": 485
    },
    {
      "text": "edema vasogénico leve",
      "type": "PATOLOGIA",
      "start": 491,
      "end": 512
    },
    {
      "text": "No hay evidencias de hemorragia intra ni extraaxial",
      "type": "HALLAZGO_NEGATIVO",
      "start": 514,
      "end": 565
    },
    {
      "text": "hipodensidad difusa en territorio cortical y subcortical",
      "type": "PATOLOGIA",
      "start": 580,
      "end": 636
    },
    {
      "text": "más marcada en regiones parasagitales y profundas bilaterales",
      "type": "LOCALIZACION_ANATOMICA",
      "start": 638,
      "end": 699
    },
    {
      "text": "sin signos de hidrocefalia",
      "type": "HALLAZGO_NEGATIVO",
      "start": 775,
      "end": 801
    },
    {
      "text": "en ambas hemisferios",
